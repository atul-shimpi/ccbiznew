I"m(function() {
  $(document).ready(function() {
    return $("#activateshop").click(function(event) {
      var activate, btntext;
      event.preventDefault();
      activate = $(this).attr("data_value");
      if (activate === 1) {
        activate = 0;
        btntext = 'Activate';
      } else {
        activate = 1;
        btntext = 'Deactivate';
      }
      return $.ajax({
        url: "/admin/shops/activate/" + $(this).attr("data_id") + "/" + activate,
        type: "get",
        success: function(data) {
          return $("activateshop").text(btntext);
        }
      });
    });
  });

}).call(this);
