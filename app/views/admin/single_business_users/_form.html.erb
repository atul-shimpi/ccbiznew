<%= form_for @singlebusinessuser, url: single_shop_events_action(@singlebusinessuser, params[:action]),:html => { :class => '' , multipart: true  } do |f| %>
<%= tinymce_assets %>
  <div class="form-group">    
    <%= f.label :first_name, :class => 'control-label' %><span class="asterix">*</span>
    <%= f.text_field :name, :class => 'text_field form-control', :placeholder => 'Enter text', :required => '' %>
  </div>
  <div class="form-group">
    <label class = 'control-label'>Middle Name<span class="asterix">*</span></label>
    <input type="text" class = 'text_field form-control' placeholder = 'Middle Name' required = '' />
  </div>
  <div class="form-group">
    <label class = 'control-label'>Last Name<span class="asterix">*</span></label>
    <input type="text" class = 'text_field form-control' placeholder ='Last Name' required = '' />
  </div>
  <div class="form-group">
    <%= f.label :phone, :class => 'control-label' %>
    <%= f.text_field :phone, :class => 'text_field form-control', :placeholder => 'Enter text'%>
  </div>
  <div class="form-group">
    <%= f.label :voting_card_number, :class => 'control-label' %>
    <%= f.text_field :votingid, :class => 'text_field form-control', :placeholder => 'Enter text' %>
  </div>
  <div class="form-group">
    <%= f.label :adhaar_number, :class => 'control-label' %>
    <%= f.text_field :adharid, :class => 'text_field form-control', :placeholder => 'Enter text' %>
  </div>
  <div class="form-group">
    <%= f.label :skills, :class => 'control-label' %>
    <%= f.text_field :skills, :class => 'text_field form-control', :placeholder => 'Enter text' %>
  </div>
  <div class="form-group">
    <%= f.label :address, :class => 'control-label' %>
    <%= f.text_field :address, :class => 'text_field form-control', :placeholder => 'Enter text' %>
  </div>
  <div class="form-group">
    <%= f.label :City, :class => 'control-label' %><span class="asterix">*</span>
    <%= f.text_field :city, :class => 'text_field form-control', :placeholder => 'Enter text', :required => '' %>
  </div>
  <div class="form-group">
      <%= f.label :state, :class => 'control-label' %><span class="asterix">*</span>
    <%= f.text_field :state, :class => 'text_field form-control', :placeholder => 'Enter text', :required => '' %>
  </div>
  <div class="form-group">
      <%= f.label :country, :class => 'control-label' %><span class="asterix">*</span>
    <%= f.text_field :country, :class => 'text_field form-control', :placeholder => 'Enter text', :required => '' %>
  </div>
  <div class="form-group">
      <%= f.label :zip, :class => 'control-label' %><span class="asterix">*</span>
    <%= f.text_field :zip, :class => 'text_field form-control', :placeholder => 'Enter text', :required => '' %>
  </div>
  
  <input type="submit" class = 'btn btn-primary' value="Submit">
  <input type="submit" class = 'btn btn-default' value="Cancel">
<% end %>
<%= tinymce plugins: ["uploadimage"] %>
