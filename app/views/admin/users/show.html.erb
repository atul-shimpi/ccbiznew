<!-- Page Content -->
<div id="page-wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header">Business User</h1>
      </div>
      <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
      <div class="col-lg-12">
        <dl class="dl-horizontal">
          <dt><strong>Name:</strong></dt>
          <dd><%= @adminsuser.full_name %></dd>
          <dt><strong>Phone:</strong></dt>
          <dd><%= @adminsuser.phone %></dd>
          <dt><strong>Email Address:</strong></dt>
          <dd><%= @adminsuser.email %></dd>          
        </dl>
        <div class="form-actions">
          <%= link_to "Back", admin_users_path, :class => 'btn btn-primary'  %>
          <%= link_to "Edit",
          edit_admin_user_path(@adminsuser), :class => 'btn btn-primary' %>
          <%= link_to "Destroy",
          admin_user_path(@adminsuser),
          :method => 'delete',
          :confirm => 'Are you sure?' ,
          :class => 'btn btn-danger' %>
        </div>
      </div>
      <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</div>
<!-- /#page-wrapper -->