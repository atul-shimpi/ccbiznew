<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>CCbizon Page Designer</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'font-awesome.min'%>
    <link href="/elements/css/build.css" rel="stylesheet">        
    <link href="/elements/css/flat-ui-pro.min.css" rel="stylesheet">    
    <link href="/elements/css/nivo-slider.css" rel="stylesheet">    
    <link href="/elements/css/skeleton.css" rel="stylesheet">    
    <link href="/elements/css/style-contact.css" rel="stylesheet">    
    <link href="/elements/css/style-content.css" rel="stylesheet">    
    <link href="/elements/css/style-dividers.css" rel="stylesheet">    
    <link href="/elements/css/style-headers.css" rel="stylesheet">    
    <link href="/elements/css/style-portfolios.css" rel="stylesheet">    
    <link href="/elements/css/style-pricing.css" rel="stylesheet">    
    <link href="/elements/css/style-team.css" rel="stylesheet">    
    <link href="/elements/css/style.css" rel="stylesheet">    
    <link href="/elements/css/video.css" rel="stylesheet">    
    <link href="/elements/css/style-footers.css" rel="stylesheet">    
    <link href="/elements/css/header-style.css" rel="stylesheet">
    <link href="/elements/css/footer-style.css" rel="stylesheet">
    <link href="/elements/css/flexslider.css" rel="stylesheet">
    <link href="/elements/css/banner-style.css" rel="stylesheet">
    <link href="/elements/css/reset.css" rel="stylesheet">
    <link href="/elements/css/services-facilities.css" rel="stylesheet">
    <link href="/elements/css/contact-form.css" rel="stylesheet">
    <%= stylesheet_link_tag    'bootstrap.min'%>
    <link href="/elements/css/services.css" rel="stylesheet">
    
    <%= javascript_include_tag 'htmlbuilder/jquery.min' %>
    <%= javascript_include_tag 'htmlbuilder/jquery-ui.min' %>
    <link rel="shortcut icon" href="images/favicon.png">
    
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements. All other JS at the end of file. -->
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->
    <!--[if lt IE 10]>
    <link href="css/ie-masonry.css" rel="stylesheet">
    <script src="js/masonry.pkgd.min.js"></script>
    <![endif]-->
</head>
<body>  
<% @header_blocks.each_with_index do |block, index| %>    
  <% block.each_with_index do |blockdetails, index| %>          
    <%= content_html blockdetails["frames_content"].to_s.html_safe %>
  <% end %>
<% end %>

<%= yield %>

<% @footer_blocks.each_with_index do |block, index| %>    
  <% block.each_with_index do |blockdetails, index| %>      
    <%= content_html blockdetails["frames_content"].to_s.html_safe %>
  <% end %>
<% end %>


    
    <!-- Load JS here for greater good =============================-->
    
    

    <%= javascript_include_tag 'htmlbuilder/flat-ui-pro.min' %>
    <%= javascript_include_tag 'htmlbuilder/chosen.min' %>
    <%= javascript_include_tag 'htmlbuilder/jquery.zoomer' %>
    <%= javascript_include_tag 'htmlbuilder/spectrum' %>
    <%= javascript_include_tag 'htmlbuilder/ace/ace' %>    
    
    <%= render :partial => "home/login_modal", locals: {} %>
</body>
</html>